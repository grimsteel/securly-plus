{
  "manifest_version": 3,
  "name": "Securly Plus",
  "description": "Dark mode and improvements for Securly Flex",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["*://flex.securly.com/*"],
      "js": ["content-script.js"],
      "css": ["dark-mode.css"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ]
}
